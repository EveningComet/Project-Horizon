config_version=5

[application]

config/name="Project Horizon"
config/description="A project intended for testing JRPG style combat. It has a sci-fi theme."
run/main_scene="res://Scenes/UI/Homebase/Homebase Home Menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SceneController="*res://Scripts/Autoloads/SceneController.gd"
PlayerPartyController="*res://Scripts/Autoloads/PlayerPartyController.gd"
EventBus="*res://Scripts/Autoloads/EventBus.gd"
PlayerInventory="*res://Scripts/Autoloads/PlayerInventory.gd"
MissionController="*res://Scripts/Autoloads/MissionController.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
Database="*res://Scripts/Autoloads/Database.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Project Horizon"

[editor_plugins]

enabled=PackedStringArray("res://addons/sound_manager/plugin.cfg", "res://addons/utility_ai/plugin.cfg")

